{
  "name": "mobicms/mobicms-classic",
  "description": "Mobile Content Management System",
  "type": "project",
  "homepage": "https://mobicms.org",
  "license": "GPL-3.0",
  "keywords": [
    "mobicms",
    "cms",
    "mobile"
  ],
  "authors": [
    {
      "name": "Oleg Kasyanov",
      "role": "Team Lead, Developer",
      "homepage": "http://mobicms.net",
